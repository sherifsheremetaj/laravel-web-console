    "homepage": "https://github.com/alkhachatryan/laravel-web-console",
    "keywords": ["Laravel", "LaravelWebConsole"],

    "require": {
            "illuminate/support": "~5.7.0|~5.8.0|^6.0|^7.0|^10.0"
    },
    
    "require-dev": {
        "phpunit/phpunit": "~7.0|~8.0|~9.0|~10.0",
        "mockery/mockery": "^1.6",
        "orchestra/testbench": "~8.0",
        "sempro/phpunit-pretty-print": "^1.4"
    },
    "autoload": {
        "files": [
            "helpers.php"
        ],
        "psr-4": {
            "Alkhachatryan\\LaravelWebConsole\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Alkhachatryan\\LaravelWebConsole\\LaravelWebConsoleServiceProvider"
            ]
        }
    }
}
